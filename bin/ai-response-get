#!/usr/bin/env python3
from json import load, dumps
from sys import stdin


def main():
    response = load(stdin)
    message = response["choices"][0]["message"]
    output = message["content"]
    if output is None:
        output = dumps(message["tool_calls"][0]["function"]["arguments"], indent=4)
    print(output)


if __name__ == "__main__":
    main()

#!/usr/bin/env python
import sys


def main():
    if len(sys.argv) != 2:
        print("Usage: python script.py <number>")
        return

    try:
        current_value = float(sys.argv[1])
    except ValueError:
        print("Please provide a valid number.")
        return

    phi = 1.618  # Golden ratio

    while current_value >= 1:
        print(current_value)
        current_value /= phi


if __name__ == "__main__":
    main()
